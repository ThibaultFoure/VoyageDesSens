<form method="post" action="{{ path('admin_session_delete', {'id': session.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer ce session ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ session.id) }}">
	<button class="btn btn-danger px-3 py-1">
		<i class="bi bi-trash text-light"></i>
	</button>
</form>
